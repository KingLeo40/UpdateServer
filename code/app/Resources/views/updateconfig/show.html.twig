{% extends 'base.html.twig' %}

{% block body %}
    <h1>Updateconfig</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ updateConfig.id }}</td>
            </tr>
            <tr>
                <th>Version</th>
                <td>{{ updateConfig.version }}</td>
            </tr>
            <tr>
                <th>Channel</th>
                <td>{{ updateConfig.channel }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ updateConfig.status }}</td>
            </tr>
            <tr>
                <th>Redirect</th>
                <td>{{ updateConfig.redirect }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('updateconfig_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('updateconfig_edit', { 'id': updateConfig.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
